<UserControl x:Class="PrismLogin.Views.SignAlr"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrismLogin.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="SignInfo" >
        <StackPanel x:Name="stack" Background="MidnightBlue" Margin="0,2,0,0"></StackPanel>
        <Grid>
            <WrapPanel HorizontalAlignment="Center">
                <Button x:Name="Getsample" Content="Sample" Width="100" Height="40" Padding="5" FontSize="20" Margin="20,0,20,0"  Click="Getsample_Click"></Button>
                <Button x:Name="Getinfo" Content="Upinfo" Width="100" Height="40" Padding="5" FontSize="20" Margin="20,0,20,0" Click="Getinfo_Click"></Button>
            </WrapPanel>
        </Grid>
    </Grid>
</UserControl>
